{application, uba,
  [{description, "A Demo of the UBA App"},
  {vsn, "1.0.0"},
  {modules, [uba_app, uba_sup, uba_reporter, generator, collector, processor, dashboard]},
  {registered, []},
  {applications, [kernel, stdlib, sasl]},
  {env, []},
  {mod, {uba_app, []}}]}.
